pipeline {
    agent any
    tools {
        jdk 'JAVA_HOME'
        nodejs 'nodejs25'
    }
    
    stages {
        
        stage ("Git Checkout") {
            steps {
                git 'https://github.com/KastroVKiran/Zomato-Project-Kastro.git'
            }
        }
     
    stage ("Build Docker Image") {
            steps {
                sh "docker build -t prashant4656/zomatoapp:1.0 ."
            }
        }
    }
}
